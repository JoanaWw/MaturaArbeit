{
	"name": "joana-maturarbeit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prep": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@material/typography": "latest",
		"@mdi/js": "latest",
		"@smui/tab": "latest",
		"@smui/tab-bar": "latest",
		"@sveltejs/adapter-auto": "latest",
		"@sveltejs/adapter-static": "latest",
		"@sveltejs/kit": "latest",
		"@sveltejs/vite-plugin-svelte": "latest",
		"smui-theme": "latest",
		"svelte": "latest",
		"svelte-check": "latest",
		"tslib": "latest",
		"typescript": "latest",
		"vite": "latest"
	},
	"type": "module",
	"dependencies": {
		"@material/dom": "latest",
		"@smui/card": "latest",
		"@smui/icon-button": "latest",
		"@smui/layout-grid": "latest",
		"@smui/top-app-bar": "latest",
		"smui": "latest"
	}
}
